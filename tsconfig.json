{
    "compilerOptions": {
        "target": "es6",
        "lib":[
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "allowsJs":true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFilesNames": true,
        "module": "esnext",
        "moduleResolution":"node",
        "resolveJsonModule":true,
        "isolatedModules":true,
        "noEmit": true,
        "jsx": "react"
    },
    "include": [
        "src"
    ]
}
  